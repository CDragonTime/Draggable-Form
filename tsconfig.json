{
  "compilerOptions": {
    "baseUrl": ".",
    "outDir": "compiler-client",
    "sourceMap": true,
    "strictNullChecks": false,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "module": "esnext",
    "target": "esnext",
    "lib": ["esnext", "dom", "dom.iterable"],
    "noImplicitAny": false,
    "moduleResolution": "node",
    "jsx": "react",
    "allowJs": true,
    "skipLibCheck": true,
    "typeRoots": ["./node_modules/@types"],
    "paths": {
      "@src/*": ["./src/*"],
      "@library/*": ["./src/*"],
      "@/*": [
        "src/*"
      ],
      "@@/*": [
        "src/.umi/*"
      ],
      "@umijs/max": [
        "/Users/chenlong/Desktop/umi-test/node_modules/_umi@4.0.71@umi"
      ],
      "@umijs/max/typings": [
        "src/.umi/typings"
      ]
    }
  },
  "include": ["./src/**/*"],
  "exclude": ["node_modules"]
}
